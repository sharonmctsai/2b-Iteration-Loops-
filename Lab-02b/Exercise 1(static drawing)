//Lab-02b
//Exercise 1(static drawing)

float x= 0;
float y=0;

void setup()
{
size(400,400);
}

void draw()
{
background(#817383);

stroke(#62CE95);
strokeWeight(5);

 x = 50;
 while(x < width)
  {
  line(x,0,x,height);
  x =x + width/8;
  }

 y = 50;
 while(y < height)
 {
 line(0,y,width,y);'
  y = y +height /8;
 }
 
}
