//While Loops Examples 2.13

int yCoordinate = 60;

size(600,300);
background(102);
fill(255);
noStroke();
int i = 0;

while(i<4)
{
 rect(50,yCoordinate,500,10);
 yCoordinate = yCoordinate +20;
 i++;
}


//While Loops Examples 2.14
Same loop, but without the yCoordinate variable


size(600,300);
background(102);
fill(255);
noStroke();


int i = 60;

while(i <=120)
{
 rect(50, i, 500 ,10);
 i+=20;
}
